<div class="popup_filter_section">
  
    <div class="modal-wrapper">
        <div class="modal-layer"></div>
        <div class="modal demo-modal">
            <button class="modal-close-btn">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 96 96" width="12" height="12"><polygon fill="currentColor" points="96,14 82,0 48,34 14,0 0,14 34,48 0,82 14,96 48,62 82,96 96,82 62,48 "></polygon></svg>
            </button>
            <div class="modal-content">
                <div id="demo-modal-content" class="demo-modal-content">

                    <div class="product_info_request_popup_header">
                        <h2>Ship <span class="request_produt_title">{{ product.title }}</span> to <span class="request_country_title"></span></h2>
                      
                      	<div class="request_product_additional_info">
                          
                            {%- for option in product.options_with_values -%}
                              <div class="request_form_product-form__input" data-req-option="{{ option.name | escape }}">
                                <label class="form__label" for="request_form_Option-{{ section.id }}-{{ forloop.index0 }}">{{ option.name }}:</label>                                
                                  <div id="request_form_Option-{{ section.id }}-{{ forloop.index0 }}"
                                    class="select__option_block"
                                    name="request_form_options[{{ option.name | escape }}]">
                                    {%- for value in option.values -%}
                                      {% if option.selected_value == value %}{{ value }}{% endif %}                                                                      
                                    {%- endfor -%}
                                  </div>                                                              
                              </div>                          
                              {%- if product.options_with_values.size > 1 -%}
                          		{%- unless forloop.last == true -%}<span class="sep">|</span>{%- endunless -%}
                              {%- endif -%}
                            {%- endfor -%}     

                      	</div>
                        <div class="request_product_info_block">                                             
                          <span class="request_product_info_label">Selected Variant: </span><span class="request_product_info"> {{ product.selected_or_first_available_variant.title }} - {{ product.selected_or_first_available_variant.price | money }}</span>
                        </div>
                      <div class="note_info"><p><em>Disclaimer: Shipping fees are at least 20%-30% of the product's original price.</em></p></div>

                      <div class="other_info"><p><strong>Submit your address information to receive a shipping quotation.</strong></p></div>

                    </div>
                    <div class="globo-formbuilder" data-id="ODI0Mzc="></div>                      
                  
                </div>
            </div>
        </div>
    </div>


</div>  
  