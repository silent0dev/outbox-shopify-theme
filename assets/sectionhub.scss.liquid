.sh-columns[data-columns][data-gap] {
  break-inside: avoid-column;
  > div {
    box-sizing: border-box;
    break-inside: avoid-column;
    overflow: hidden;
    display: inline-block;
  }
  &[data-columns="2"] {
    column-count: 2;
    @media (max-width: 600px) {
        column-count: 1;
    }
  }
  &[data-columns="3"] {
    column-count: 3;
    @media (max-width: 991px) {
        column-count: 2;
    }
    @media (max-width: 600px) {
        column-count: 1;
    }
  }
  &[data-columns="4"] {
    column-count: 4;
    @media (max-width: 1300px) {
        column-count: 3;
    }
    @media (max-width: 991px) {
        column-count: 2;
    }
    @media (max-width: 600px) {
        column-count: 1;
    }
  }
  &[data-columns="5"] {
    column-count: 5;
    @media (max-width: 1300px) {
        column-count: 4;
    }
    @media (max-width: 1100px) {
        column-count: 3;
    }
    @media (max-width: 800px) {
        column-count: 2;
    }
    @media (max-width: 500px) {
        column-count: 1;
    }
  }
  &[data-gap="0"] {
    @include sh_grid(0px);
  }        
  &[data-gap="1"] {
    @include sh_grid(1px);
  }        
  &[data-gap="2"] {
