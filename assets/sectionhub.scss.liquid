                }
                &[data-columns="3"] {
                    width: 100%*3/4;
                }
                &[data-columns="4"] {
                    width: 100%;
                }
            }
        }
        &[data-columns="5"] {
            > .sh-item {
                &[data-columns="1"] {
                    width: 100%*1/5;
                }
                &[data-columns="2"] {
                    width: 100%*2/5;
                }
                &[data-columns="3"] {
                    width: 100%*3/5;
                }
                &[data-columns="4"] {
                    width: 100%*4/5;
                }
                &[data-columns="5"] {
                    width: 100%;
                }
