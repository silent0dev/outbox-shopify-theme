.sh-julia-lookbook {
  @include sh_section_header;
  margin-top: 50px;
  margin-bottom: 100px;
  > .sh-header {
    margin-bottom: 40px;
  }
  .sh-grid {
    .sh-item {
      position: relative;
      > .sh-media {
        z-index: -1;
        position: relative;
      }
      > .sh-pickers {
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        .sh-picker {
          position: absolute;
          transform: translate(-50%, -50%);
          .sh-dot {
            position: relative;
            z-index: 0;
            display: block;
            text-align: center;
            border-radius: 50%;
            height: 15px;
            width: 15px;
            line-height: 15px;
            box-shadow: 0 0 0 8px rgba(0,0,0,0.1);
            background-color: {{ settings.sh_brand_color }};
            &::before {
              content: "";
              position: absolute;
              top: 50%;
              left: 50%;
              transform: translate(-50%, -50%);
              width: 6px;
              height: 6px;
              display: block;
              border-radius: 50%;
              background-color: #fff;
            }
            &::after {
              box-shadow: 0 0 0 8px rgba(255,255,255,0.3);
              border-radius: 200%;
              content: "";
              position: absolute;
              z-index: -1;
              transform: scale(1);
              animation: sh_hotspot_pulse 2s infinite;
              top: 0;
              right: 0;
              bottom: 0;
              left: 0;
            }
          }
          &:hover {
            .sh-julia-product-mini-card {
              display: flex;
            }
          }
          &.sh-above.sh-left {
            .sh-julia-product-mini-card {
              transform: translate(-100%, -100%);
            }                
          }
          &.sh-above.sh-right {
            .sh-julia-product-mini-card {
              transform: translate(0%, -100%);
            }                
          }
          &.sh-below.sh-left {
            .sh-julia-product-mini-card {
              transform: translate(-100%, 0%);
            }                
          }
          &.sh-below.sh-right {
            .sh-julia-product-mini-card {
              transform: translate(0%, 0%);
            }                
          }
          .sh-julia-product-mini-card {
            display: none;
            position: absolute;
            z-index: 1;
            top: 50%;
            left: 50%;
            width: 200px;
            background-color: white;
            padding: 15px;
            flex-direction: column;
            align-items: flex-start;
            border: 1px solid $border_light_color;
            .sh-media {              
              margin-bottom: 10px;
            }
            .sh-data {
              padding-left: 0;
            }
          }
        }
      }
    }
  }
}

@keyframes sh_pulse {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.5;
  }
  100% {
    opacity: 1;
  }
}


@keyframes sh_hotspot_pulse {
  0% {
    transform: scale(1);
    opacity: 1;
  }
  100% {
    transform: scale(1.5);
    opacity: 0;
  }
}

@keyframes sh_spin {
    from {
        transform: rotate(0deg)
    }

    to {
        transform: rotate(360deg)
    }
}

@keyframes sh_checkmark {
    0% {
        height: 0;
        width: 0;
        opacity: 1
    }

    20% {
        height: 0;
        width: 7px;
        opacity: 1
    }

    40% {
        height: 14px;
        width: 7px;
        opacity: 1
    }

    90% {
        height: 14px;
        width: 7px;
        opacity: 1
    }

    100% {
        height: 14px;
        width: 7px;
        opacity: 0
    }
}

@keyframes sh_checkmark_big {
    0% {
        height: 0;
        width: 0;
        opacity: 1
    }

    20% {
        height: 0;
        width: 10px;
        opacity: 1
    }

    40% {
        height: 22px;
        width: 10px;
        opacity: 1
    }

    90% {
        height: 22px;
        width: 10px;
        opacity: 1
    }

    100% {
        height: 22px;
        width: 10px;
        opacity: 0
    }
}

@keyframes sh_slideout {
    100% {
        width: 0
    }
}

@keyframes sh_fadeInUp {
  from {
    opacity: 0;
    transform: translate3d(0, 100%, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}

@keyframes sh_fadeOutDown {
  from {
    opacity: 1;
  }

  to {
    opacity: 0;
    transform: translate3d(0, 100%, 0);
  }
}

@keyframes sh_video_scale {
  0% {
    transform: scale(1); 
  }
  100% {
    transform: scale(1.8);
    opacity: 0; 
  } 
}

