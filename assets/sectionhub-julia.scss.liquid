@mixin sh_table($padding) {
  table {
    thead {
      th {
        text-transform: uppercase;
      }
    }      
    thead, tbody, tfoot {
      tr {
        th,td {
          padding: $padding;
          border: 1px solid {{ settings.sh_border_color }};
        }         
      }
    }
  }
}

@mixin sh_overlay($opacity: false) {
  position: relative;
  z-index: 0;
  > .sh-overlay {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-position: center;
    background-size: cover;
    pointer-events: none;
    z-index: -1;
    @if $opacity {
    	opacity: $opacity;
  	}    
  }
}

@mixin sh_form {
  input, textarea, select {
    @include sh_control;
    width: 100%;
    box-sizing: border-box;
  }
  button, input[type="submit"] {
    @include sh_button;    
  }
