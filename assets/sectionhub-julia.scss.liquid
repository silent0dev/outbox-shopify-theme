.sh-julia-menu-1 {
  $menu-icons-color: {{ settings.sh_header_color }};
  $menu-border-color: {{ settings.sh_header_color }};
  $dropdown-border-radius: 0;
  $megamenu-bold: 600;
  $menu-transition: all .4s ease;
  
  $menu-font: {{ settings.sh_paragraph_family.family }};
  $menu-font-size: 14px;
  $menu-font-color: {{ settings.sh_header_color }};
  $menu-font-weight: 500;
  $menu-letter-spacing: 0;

  $submenu-font: {{ settings.sh_paragraph_family.family }};
  $submenu-font-size: 14px;
  $submenu-color: {{ settings.sh_header_color }};
  $submenu-font-weight: 500;
  $submenu-line-height: 1.2;
  $submenu-letter-spacing: 0;
  $submenu-padding: 23px;
  
  $menu-item-v-padding: 5px;
  $menu-item-h-padding: 15px;
  $dropdown-shadow: 0px 2px 10px 0 transparentize({{ settings.sh_header_color }}, 0.95);

  $overlay-menu-font: {{ settings.sh_paragraph_family.family }};
  $overlay-menu-font-size: 30px;
  $overlay-menu-color: {{ settings.sh_header_color }};
  $overlay-menu-font-weight: 400;
  $overlay-menu-line-height: 1.5;
  $overlay-menu-letter-spacing: 0;  
  $overlay-menu-bgcolor: transparentize($overlay-menu-color, 0.1);

  $menu-shift: 10px;
  $sub-menu-border-radius: $dropdown-border-radius;

  @mixin mega-menu {
    @media (min-width: $menu-mobile-width) {
      .sh-sub-menu.sh-mega-menu {
        display: flex;
        flex-wrap: wrap;
        padding: 30px 10px;
        box-sizing: border-box;
        > .sh-menu-item {
          width: calc(25% - 0px);
          margin: 0;
          border-bottom: 0;
          white-space: normal;   
          &::after {
            content: none;
          }
          a {                        
            vertical-align: middle;
          }
          > a {                        
            font-weight: $megamenu-bold;
          }
          > label {
            display: none;
          }
          > .sh-sub-menu {
            position: static;
            top: auto !important; 
            left: auto !important;
            min-width: auto !important;         
            background: transparent !important;
            box-shadow: none !important;
          }
        }
      }
      &.sh-mega-menu:hover {
        .sh-sub-menu {
          visibility: visible !important;
          opacity: 1 !important;
        }
        .sh-sub-menu:not(.sh-mega-menu) {
          display: block;
        }
      }
    }
  }
  @mixin menu {
    min-height: $menu-line-height;
    pointer-events: none;
    display: inline-block;
    .sh-menu-root {
      display: flex;
      > .sh-menu-item {
        pointer-events: all;
        position: relative;
        &.sh-has-children > .sh-menu-link .sh-down-icon {
          position: relative;
          top: 1px;
          margin-left: 5px;
          color: $menu-icons-color;
          transition: $menu-transition;
          svg {
            height: $submenu-font-size - 4px;
          }
        }
        &:hover {
          &.sh-has-children > .sh-menu-link::after {
            color: {{settings.sh_brand_color}};
          }                
        }
        &:first-child {
          > .sh-menu-link {
            padding-left: 0;
          }
          > .sh-sub-menu {
            left: -$submenu-padding - $menu-item-h-padding;//right:0;
          }
        }
        &:last-child {
          > .sh-menu-link {
            padding-right: 0;
          }
        }
        @include mega-menu;
      }     
      &::after {
        content:"";
        display:table;
        clear:both;
      }
    }
    .sh-menu-root, .sh-sub-menu {
      list-style:none;
      position:relative;
      > .sh-menu-item {
        position:relative;
        &:hover > .sh-sub-menu {
          visibility:visible;
          opacity:1;
          transition: $menu-transition;
        }
        &:hover, &.sh-current {
          > .sh-menu-link {
            color: {{settings.sh_brand_color}};
          }
        }
        > .sh-menu-link {
          position:relative;
          transition: $menu-transition;
          color: $menu-font-color;
          display:block;
          text-decoration:none;
          line-height: $menu-line-height;
          padding: 0 $menu-item-h-padding;
          font-size: $menu-font-size;
          font-weight: $menu-font-weight;
          letter-spacing: $menu-letter-spacing;
          vertical-align: middle;
          font-family: $menu-font;
          white-space: nowrap;
        }
        > .sh-sub-menu {             
          transition: $menu-transition;
          visibility:hidden;
          opacity:0;

          position:absolute;
          top: calc(100% - #{$menu-shift});
          left: -$submenu-padding;//right:0;
          background-color: white;
          padding: $submenu-padding 0;
          text-align: left;//text-align: right;
          box-shadow: $dropdown-shadow;
          border-radius: $sub-menu-border-radius;
          > .sh-menu-item {
            min-width: 240px; 
            &.sh-has-children .sh-right-icon {
              position: absolute;
              top: 50%;
              right: $submenu-padding;
              transform: translateY(-50%);
              height: $submenu-font-size - 4px;
              color: $menu-icons-color;
              transition: $menu-transition;
              svg {
                height: $submenu-font-size - 4px;
                display: block;
              }
            }
            &:hover, &.sh-current {
              &.sh-has-children::after {
                color: {{settings.sh_brand_color}};
              }
              > .sh-menu-link {
                color: {{settings.sh_brand_color}};                                
              }                        
              > .sh-sub-menu {
                display: block;
              }
            }
            > .sh-menu-link {
              transform: translate(0, 0);
              transition: $menu-transition;
              font-family: $submenu-font;
              font-size: $submenu-font-size;
              font-weight: $submenu-font-weight;
              letter-spacing: $submenu-letter-spacing;
              line-height: $submenu-line-height;
              color: $submenu-color;
              padding: $menu-item-v-padding ($menu-item-h-padding + $submenu-padding);
              text-transform: none;
              white-space: nowrap;
              vertical-align: middle;
            }
            > .sh-sub-menu {
              top: -$submenu-padding;
              left: 100%;//right:100%;
              margin-left: 0px;
              display: none;
              border-radius: 0 $dropdown-border-radius $dropdown-border-radius 0;
              border-top-left-radius: 0 !important;
              border-bottom-left-radius: 0 !important;
            }
          }
        } 
        > [type="checkbox"], > label {
          display: none;
        }
      }
    }
  }
  @mixin overlay-menu {
    pointer-events: none;
    opacity: 0;
    transition: $menu-transition;
    position: fixed;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    z-index: 1;
    background-color: $overlay-menu-bgcolor;
    .sh-menu-root {
      position: absolute;
      top: 20vh;
      max-height: 70vh;
      box-sizing: border-box;
      left: 50%;
      width: 100%;
      overflow-y: auto;
      max-width: 500px;
      padding: 30px;
      transform: translateX(-50%); 
      color: $overlay-menu-color;
      font-family: $overlay-menu-font; 
      font-size: $overlay-menu-font-size;
      line-height: $overlay-menu-line-height;
      font-weight: $overlay-menu-font-weight;
      letter-spacing: $overlay-menu-letter-spacing;
      -webkit-font-smoothing: auto;
      font-size: $overlay-menu-font-size *0.79;
      color: white;
      .sh-menu-item {
        position:relative;
        &.sh-has-children {                    
          > [type="checkbox"] {
            position:absolute;
            clip:rect(0 0 0 0);
            &:checked {
              ~ label {
                .sh-down-icon {
                  display: none;
                }
                .sh-up-icon {
                  display: inline;
                }
              }
              ~ .sh-sub-menu {
                display: block;
              }
            }
          }
          > label {
            font-size: 0.5em;
            position:absolute;
            right: 0;
            top: 0;
            width: 3em;
            line-height: 300%;
            vertical-align: middle; 
            text-align: center;
            cursor: pointer;
            transition: $menu-transition;
            .sh-down-icon, .sh-up-icon {
              svg {
                height: $overlay-menu-font-size - 4px;
                vertical-align: middle;
              }
            }
            .sh-down-icon {
              display: inline;
            }
            .sh-up-icon {
              display: none;
            }
          }
        }
        &.sh-current-ancestor {
          > label {
            .sh-down-icon {
              display: inline;
            }
            .sh-up-icon {
              display: none;
            }
          }
          > .sh-sub-menu {
            display: block;
          }                    
        }
        .sh-menu-link {
          display: block;
          padding-left: 10px;
          padding-right: 40px;
          margin-bottom: 10px;
          transition: $menu-transition;
          color: white;
          .sh-down-icon, .sh-right-icon {
            display: none;
          }
        }
        &.sh-current {
          > .sh-menu-link {
            background-color: {{settings.sh_brand_color}};
            color: white !important;                    
          }
        }
        .sh-sub-menu {
          display: none;
          padding-left: 20px; 
        }
      }
    }
  }  

  .sh-menu-element {
    position: relative;
    z-index: 2;
    > [type="checkbox"] {
      position:absolute;
      clip:rect(0 0 0 0);
    }
    @mixin desktop {
      > label {
        display: none;
      }
      .sh-menu-wrapper {
        @include menu;
      }
    }
    @mixin mobile {
      .sh-menu-wrapper {
        @include overlay-menu;
        text-align: left;
      }
      > [type="checkbox"]:checked {
        ~ .sh-menu-wrapper {
          pointer-events: none;
          opacity: 1;
          z-index: 2;
          .sh-menu-root {
            pointer-events: all;                                
          }                            
        }
        ~ label {
          pointer-events: all;
          position: fixed;
          left: 0;
          top: 0;
          right: 0;
          bottom: 0;
          span {
          }
        }
      }            
    }
    @media (min-width: $menu-mobile-width) {
      @include desktop;
    }
    @media (max-width: $menu-mobile-width) {
      @include mobile;
    }        
  }
  [data-sticky-class="sh-sticky-menu"] {
    box-shadow: 0px 50px 50px 0 transparentize({{ settings.sh_header_color }}, 0.98);	
    @media (max-width: $menu-mobile-width) {
      padding: 5px 15px;
    }
  }
