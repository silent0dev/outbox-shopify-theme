.sh-julia-product-card {
  transition: $transition;
  box-shadow: none;
  background-color: white;
  overflow: hidden;
  .sh-media {
    position: relative;
    .sh-swiper-container {
      .sh-swiper-slide {
        .sh-image {          
        }
      }
      .sh-swiper-button-prev, .sh-swiper-button-next {
        transition: $transition;
        opacity: 0;
        top: calc(50% - 20px);
      }
    }
    .sh-image {
      margin: auto;
      max-width: 100%;
    }
    @include sh_stickers;
  }
  .sh-data {			
    padding: 15px 20px;        
    position: relative;
    transition: $transition;
    z-index: 1;
    background-color: white;
    .sh-header {			
      .sh-extra {
        text-transform: uppercase;
        font-size: 12px;
        color: #aaa;
        margin-bottom: 5px;
      }
      .sh-title {		
        font-size: 15px;
        line-height: 1.3;
        font-weight: 400;
        margin-bottom: 5px;
      }
      .sh-price-wrapper {
        font-size: 12px;
        font-weight: 600;
        .sh-price-compare {
		  text-decoration: line-through;
          font-weight: 400;
        }
        .sh-unit-price {
          color: #aaa;
        }
      }
    }
    .sh-footer {
      position: absolute;
      left: 20px;
      right: 20px;
      top: calc(100% + 2px);
      display: flex;
	  justify-content: space-between;
      flex-wrap: nowrap;
      .sh-add-to-cart, .sh-select-options {
        cursor: pointer;
        text-transform: uppercase;
    	font-size: 12px;
    	font-weight: 600;
        letter-spacing: 1px;
        transition: $transition;
        transition-delay: .1s;
        transform: translateY(50%);
        position: relative;
        display: flex;
    	align-items: center;
        &::before {
          content: "";
          display: inline-block;
          width: 0;
          height: 16px;
          opacity: 0;
        }
        &::after {
        }
        &.sh-adding {         
          &::before {
            opacity: 1;
            width: 16px;
            margin-right: 10px;
            border-radius: 50%;
            border: 2px solid;
            border-top-color: transparent;
            animation: sh_spin infinite 1s linear;
            transition: opacity .2s linear .2s
          }
        }
        &.sh-added {
          &::before {
            width: 30px;
			animation: sh_slideout .2s linear 2s forwards
          }
          &::after {
            content: "";
            position: absolute;
            top: 10px;
            left: 2px;
            border-top: 2px solid;
            border-right: 2px solid;
            transition: none;
            transform: scaleX(-1) rotate(135deg);
            transform-origin: left top;
            animation: sh_checkmark 2s ease forwards
          }
        }
      }
      .sh-add-to-wishlist {
		transition: $transition;
        transition-delay: .1s;
        transform: translateY(50%);
        font-size: 0;
        display: flex;
    	align-items: center;
        span {
          display: none;
          svg {
            height: 20px;
          }          
        }
        &.sh-added {
          .sh-added {
            display: inline;
          }
        }
        &:not(.sh-added) {
          .sh-add {
            display: inline;
          }
        }
        .sh-add, .sh-added {
          cursor: pointer;
        }
        .sh-adding {
          &::before { 
            content: "";
            vertical-align: 2px;
            display: inline-block;
            height: 16px;
            width: 16px;
            border-radius: 50%;
            border: 2px solid;
            border-top-color: transparent;
            animation: sh_spin infinite 1s linear;
            transition: opacity .2s linear .2s
          }
        }
      }
    }
  }
  &:hover {
    box-shadow: 0px 8px 32px 0px rgba(0,0,0,.1);
    .sh-media {
      .sh-swiper-container {
        .sh-swiper-button-prev, .sh-swiper-button-next {
          opacity: 1;
        }
      }
    }
    .sh-data {
      transform: translateY(-40px);
      .sh-footer {
        .sh-add-to-cart, .sh-select-options, .sh-add-to-wishlist {
          transform: translateY(0%);
        }
      }
    }    
  }
}

