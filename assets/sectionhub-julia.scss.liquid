{{ settings.sh_paragraph_family | font_modify: 'weight', '500' | font_face }}
{{ settings.sh_paragraph_family | font_modify: 'weight', '600' | font_face }}

$transition: .3s all ease-in-out;
$border_light_color: #eee;
$whitesmoke_color: transparentize(black, 0.96);
$blacksmoke_color: transparentize(white, 0.9);
$page_header_font_size: {{ settings.sh_header_font_size }}px * 1.333;
$element_font_size: {{ settings.sh_header_font_size }}px * 0.533;
$menu-mobile-width: 1199px;
$menu-line-height: 80px;

@mixin placeholder($property, $value) {
    &:placeholder {
        #{$property}: $value;
    }
    &::placeholder {
        #{$property}: $value;
    }
    &::-webkit-input-placeholder {
        #{$property}: $value;
    }
    &::-moz-placeholder {
        #{$property}: $value;
    }
    &:-moz-placeholder {
        #{$property}: $value;
    }
    &:-ms-input-placeholder {
     	#{$property}: $value;
    }     
}

@mixin sh_control {
  padding: 9px 15px;
  border: 1px solid {{ settings.sh_border_color }};
  border-radius: 0;
  background-image: none !important;
  background-color: white;
  font-size: 14px;
  @include placeholder(color, #aaa);
}

@mixin sh_link {  
  color: {{ settings.sh_header_color }};
  transition: $transition;  
  &:hover {
    color: {{ settings.sh_brand_color }};
  }
}

@mixin sh_inverted_link {  
  color: white;
  transition: $transition;  
  &:hover {
    color: white;
    text-decoration: underline;
  }
}

@mixin sh_header {
    font-family: {{ settings.sh_header_family.family }};
    font-weight: {{ settings.sh_header_family.weight }};
    font-style: {{ settings.sh_header_family.style }};      
    color: {{ settings.sh_header_color }};
    font-size: {{ settings.sh_header_font_size }}px;
    line-height: {{ settings.sh_header_line_height }};
}


@mixin sh_button {
  @include sh_control;
  //padding: 9px 30px;
  //border-radius: 20px;
  cursor: pointer;
  width: auto;
  background-color: {{ settings.sh_button_color }};
  border-color: {{ settings.sh_button_color }};
  color: white;
  transition: $transition;
  display: inline-block;
  white-space: nowrap;
  &:hover {
    background-color: white;
    color: {{ settings.sh_header_color }};
  }
  &::before {
    content: none;
  }
}

@mixin sh_secondary_button {
  @include sh_button;
  background-color: white;
  color: {{ settings.sh_header_color }};
  &:hover {
    background-color: {{ settings.sh_button_color }};
    color: white;
  }
}

@mixin sh_inverted_button {
  @include sh_control;
  background-color: white;
  border-color: white;
  color: {{ settings.sh_header_color }};
  transition: $transition;
  display: inline-block;
  white-space: nowrap;
  &:hover {
    background-color: {{ settings.sh_brand_color }};
    border-color: {{ settings.sh_brand_color }};
    color: white;
  }
}

@mixin sh_text_button {  
  font-weight: 600;
  font-size: 15px;
  letter-spacing: 0.5px;
  display: inline-flex;
  white-space: nowrap;
  align-items: center;
  color: {{ settings.sh_header_color }};
  span {
    color: {{ settings.sh_header_color }};
  }
  .sh-text {
    margin-right: 5px;
  }                
  .sh-icon {
    font-size: 0;
    svg {
      width: 20px;
      height: 20px;
    }
  }
  border-bottom: 1px solid transparent;
  position: relative;
  &::before {
    content: '';
    position: absolute;
    transition: $transition;
    left: 0;
    right: 0;
    bottom: -4px;
    height: 0;
    border-bottom: 1px solid {{ settings.sh_header_color }};
    opacity: 0;
    transform: translateY(5px);
  }
  &:hover {
    &::before {
      opacity: 1;
      transform: translateY(0);
    }
  }            
}

@mixin sh_inverted_text_button {
  @include sh_text_button;
  color: white;
  span {
    color: white;
  }
  &::before {
    border-color: white;
  }
}

@mixin sh_author {
  display: flex;
  align-items: center;
  .sh-image {
    border-radius: 100%;
    margin-right: 15px;
    flex-shrink: 0;
  }
  .sh-data {
    .sh-name {
      font-weight: bold;
      font-size: 15px;
    }
    .sh-title {
      text-transform: uppercase;
      font-size: 11px;
      margin-top: 5px;
    }
  }
}

@mixin sh_rating {
  display: flex;
  align-items: center;
  color: {{ settings.sh_brand_color }};
  margin-left: -5px;
  .sh-icon {
    svg {
      height: 20px;
    }
  }
}
  
@mixin sh_tabs {
  > .sh-tabs {
    > .sh-titles {
      margin-bottom: 30px;
      > span {
        font-size: 15px;
    	text-transform: uppercase;
        display: inline-block;
        margin-right: 5px;
        padding: 5px 10px;
        cursor: pointer;
        transition: $transition;
        border-bottom: 1px solid transparent;
        position: relative;
        &::before {
          content: '';
          position: absolute;
          transition: $transition;
          left: 0;
          right: 0;
          bottom: -1px;
          height: 0;
          border-bottom: 1px solid {{ settings.sh_header_color }};
          opacity: 0;
          transform: translateY(5px);
        }
        &:hover {
          &::before {
            opacity: 1;
            transform: translateY(0);
          }
        }
        display: inline-flex;
        .sh-icon {
          display: flex;
          margin-right: 5px;
          svg {
            height: 1.5em;
          }
        }
      }
      .sh-active {
        border-bottom: 1px solid {{ settings.sh_header_color }};
      }
    }
    > .sh-content {
      > .sh-item {
      }
    }
  }
}

@mixin sh_inverted_tabs_ {
  > .sh-tabs {
    > .sh-titles {
      span {
        color: white;
        &::before {
          border-color: white;
        }
      }
      .sh-active {
        border-color: white;
      }
    }
    > .sh-content {
      > .sh-item {
      }
    }
  }
}

